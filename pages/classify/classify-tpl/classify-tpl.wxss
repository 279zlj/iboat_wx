.shipimg{
  height: 200rpx;
  width: 360rpx;
}
.shiptext{
  display: flex;
  flex-direction: column;
  width: 250rpx;
  font-size: 32rpx;
 
}
.shiptitle,.shipcontent,.shipmoney{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 350rpx;
  line-height: 60rpx;
}
.shipmoney{
  color: red
}
.ship_img{
  height: 240rpx;
  width: 295rpx;
  border-radius: 5px;
  margin: 20rpx;
}
.ship_text{
  display: flex;
  flex-direction: column;
  font-size:16px;
  width: 100%;
  
  
}
.title{
  letter-spacing: 2rpx;
  font-weight: 600;
  margin: 5px 0 5px 10px;
  font-size: 32rpx;
}
.money{
  color: red;
  font-size: 30rpx;
  margin: 5px 0 15px 10px;
}
.content{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 300rpx;
  margin: 10rpx 0 30rpx 20rpx;
  font-size: 28rpx;
}
.classify_one{
  display: flex;
  flex-direction: column;
  border-radius: 150rpx;
  border:2px #ccc solid;
  width: 28%;
  margin: 30rpx 0 0 25rpx;
  height: 200rpx;
  background: white;
}
.classify_one image{
  width: 100rpx;
  height: 90rpx;
  
  margin: 10rpx auto;
}
.classify_one text{
  text-align: center;
  font-size: 32rpx;
}